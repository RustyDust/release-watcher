      {
        "date": "2018-03-16",
        "type": "eap",
        "downloads": {
          "linux": {
            "link": "https://download.jetbrains.com/go/goland-181.4203.206.tar.gz",
            "size": 214798968,
            "checksumLink": "https://download.jetbrains.com/go/goland-181.4203.206.tar.gz.sha256"
          },
          "mac": {
            "link": "https://download.jetbrains.com/go/goland-181.4203.206.dmg",
            "size": 204576764,
            "checksumLink": "https://download.jetbrains.com/go/goland-181.4203.206.dmg.sha256"
          },
          "windows": {
            "link": "https://download.jetbrains.com/go/goland-181.4203.206.exe",
            "size": 162003200,
            "checksumLink": "https://download.jetbrains.com/go/goland-181.4203.206.exe.sha256"
